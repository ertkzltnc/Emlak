        #hata {
            color: red;
            font-size: 16px;
        }

        #hatasız {
            color: green;
            font-size: 16px;
        }

        #sifrehatasi {
            color: red;
            font-size: 16px;
        }
    </style>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="/Content//Content/assets/images/required/ico/favicon.ico">
    <!-- Fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="/Content/assets/images/required/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="/Content/assets/images/required/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="/Content/assets/images/required/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="/Content/assets/images/required/ico/apple-touch-icon-57-precomposed.png">
    <link rel="shortcut icon" href="/Content/assets/images/required/ico/favicon.png">
    <title>Emlak</title>
    <!-- Required CSS Files -->
    <link type="text/css" href="/Content/assets/css/required/bootstrap/bootstrap.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,300&subset=latin,latin-ext' rel='stylesheet' type='text/css'>
    <link type="text/css" href="/Content/assets/js/required/jquery-ui-1.11.0.custom/jquery-ui.min.css" rel="stylesheet" />
    <link type="text/css" href="/Content/assets/js/required/jquery-ui-1.11.0.custom/jquery-ui.structure.min.css" rel="stylesheet" />
    <link type="text/css" href="/Content/assets/js/required/jquery-ui-1.11.0.custom/jquery-ui.theme.min.css" rel="stylesheet" />
    <link type="text/css" href="/Content/assets/css/required/mCustomScrollbar/jquery.mCustomScrollbar.min.css" rel="stylesheet" />
    <link type="text/css" href="/Content/assets/css/required/icheck/all.css" rel="stylesheet" />
    <link type="text/css" href="/Content/assets/fonts/metrize-icons/styles-metrize-icons.css" rel="stylesheet">
    <!-- Optional CSS Files -->
    <link type="text/css" href="/Content/assets/css/optional/bootstrapValidator.min.css" rel="stylesheet" />
    <!-- add CSS files here -->
    <!-- More Required CSS Files -->
    <link type="text/css" href="/Content/assets/css/styles-core.css" rel="stylesheet" />
    <link type="text/css" href="/Content/assets/css/styles-core-responsive.css" rel="stylesheet" />
    <!-- Demo CSS Files -->
    <link type="text/css" href="/Content/assets/css/demo-files/pages-signin-signup.css" rel="stylesheet" />
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <script src="/Content/assets/js/required/misc/ie10-viewport-bug-workaround.js"></script>
    <!--[if IE 7]>
    <link type="text/css" href="/Content/assets/css/required/misc/style-ie7.css" rel="stylesheet">
    <script type="text/javascript" src="/Content/assets/fonts/lte-ie7.js"></script>
    <![endif]-->
    <!--[if IE 8]>
    <link type="text/css" href="/Content/assets/css/required/misc/style-ie8.css" rel="stylesheet">
    <![endif]-->
    <!--[if lte IE 8]>
    <script type="text/javascript" src="/Content/assets/css/required/misc/excanvas.min.js"></script>
    <![endif]-->
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="/Content/https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="/Content/https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div class="container-fluid">
        <div id="body-container">
            <div class="standalone-page registration-page">
                <div class="standalone-page-top-border">
                    <span class="border-block bg-color-green"></span>
                    <span class="border-block bg-color-orange"></span>
                    <span class="border-block bg-color-yellow"></span>
                    <span class="border-block bg-color-blue"></span>
                    <span class="border-block bg-color-red"></span>
                    <span class="border-block bg-color-lime"></span>
                    <span class="border-block bg-color-purple"></span>
                </div>
                <div class="standalone-page-block">
                    <div class="row">
                        <div class="col-xs-12">
                            <h2 class="heading">
                                <span aria-hidden="true" class="icon icon-user"></span>
                                <span class="main-text">
                                    Yeni Ofis Kayıt Olustur
                                </span>
                            </h2>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <form role="form" class="registration-form form-horizontal" method="post" action="/EmlakOfisi/EmlakOfisiEkle">
                                <div class="form-group">
                                    <label for="inputAd" class="col-sm-3 control-label">Ofis Adı</label>
                                    <div class="col-sm-9">
                                        <input autocomplete="off" class="form-control" id="inputAd" placeholder="Ofis Adı" type="text" name="Ad" required>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputYetkili" class="col-sm-3 control-label">Yetkili Adı</label>
                                    <div class="col-sm-9">
                                        <input autocomplete="off" class="form-control" id="inputYetkili" placeholder="Yetkili Adı" type="text" name="Yetkili" required>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputIl" class="col-sm-3 control-label">Il Adı</label>
                                    <div class="col-sm-9">

                                        <select class="form-control" id="IlDropDown" name="IlID" onchange="IlceListele()">
                                            @foreach (Il il in ViewBag.iLList)
                                            {
                                                <option value="@il.ID">@il.Ad</option>
                                            }
                                        </select>

                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputIlce" class="col-sm-3 control-label">Ilce Adı</label>
                                    <div class="col-sm-9">
                                        <select class="form-control" id="IlceDropDown" name="IlceID" onchange="MahalleListele()"></select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputMahalle" class="col-sm-3 control-label">Mahalle Adı</label>
                                    <div class="col-sm-9">
                                        <select class="form-control" id="MahalleDropDown" name="MahalleID"></select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputAdres" class="col-sm-3 control-label">Adres</label>
                                    <div class="col-sm-9">
                                        <input autocomplete="off" class="form-control" id="inputAdres" placeholder="Adres" type="text" name="AdresAyrinti" required>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputEmail" class="col-sm-3 control-label">E-posta</label>
                                    <div class="col-sm-9">
                                        <input autocomplete="off" class="form-control" id="inputEmail" placeholder="E-Posta" type="email" name="Eposta" required>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputTelefon" class="col-sm-3 control-label">Telefon</label>
                                    <div class="col-sm-9">
                                        <input autocomplete="off" class="form-control" id="inputTelefon" placeholder="Telefon" type="text" name="CepTelefonu" pattern="\d{11}" required>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputFax" class="col-sm-3 control-label">Fax</label>
                                    <div class="col-sm-9">
                                        <input autocomplete="off" class="form-control" id="inputFax" placeholder="Fax" type="text" pattern="\d{11}" name="Fax" required>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputSifre" class="col-sm-3 control-label">Sifre</label>
                                    <div class="col-sm-9">
                                        <input autocomplete="off" class="form-control" id="inputSifre" placeholder="Sifre" type="password" name="Sifre" onkeyup="validate()">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputSifreTekrar" class="col-sm-3 control-label">Sifreyi Tekrarla</label>
                                    <div class="col-sm-9">
                                        <input autocomplete="off" class="form-control" id="inputSifreTekrar" placeholder="Sifre" type="password" name="SifreTekrar" onkeyup="validate()">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-offset-3 col-sm-9">
                                        <button id="hesapOlustur" type="submit" class="btn btn-success">Hesap Oluştur</button>
                                    </div>
                                </div>
                            </form>
                            <span id="hata">@ViewBag.CreateMessage</span>
                            <span id="hatasız">@ViewBag.Message</span>
                            <span id="sifrehatasi">@ViewBag.Message2</span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="change-section">
                                <h3 class="heading">Bir Hesabın Zaten Var mı?</h3>
                                <a href="/EmlakOfisi/YetkiliGiris" class="btn btn-default btn-block">Giriş Yap</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div><!-- /.container -->
    <!-- Placed at the end of the document so the pages load faster -->
    <!-- Required JS Files -->
    <script type="text/javascript" src="/Content/assets/js/required/jquery-1.11.1.min.js"></script>
    <script type="text/javascript" src="/Content/assets/js/required/jquery-ui-1.11.0.custom/jquery-ui.min.js"></script>
    <script type="text/javascript" src="/Content/assets/js/required/bootstrap/bootstrap.min.js"></script>
    <script type="text/javascript" src="/Content/assets/js/required/jquery.easing.1.3-min.js"></script>
    <script type="text/javascript" src="/Content/assets/js/required/jquery.mCustomScrollbar.min.js"></script>
    <script type="text/javascript" src="/Content/assets/js/required/misc/jquery.mousewheel-3.0.6.min.js"></script>
    <script type="text/javascript" src="/Content/assets/js/required/misc/retina.min.js"></script>
    <script type="text/javascript" src="/Content/assets/js/required/icheck.min.js"></script>
    <script type="text/javascript" src="/Content/assets/js/required/misc/jquery.ui.touch-punch.min.js"></script>
    <script type="text/javascript" src="/Content/assets/js/required/circloid-functions.js"></script>
    <!-- Optional JS Files -->
    <script type="text/javascript" src="/Content/assets/js/optional/bootstrapValidator.min.js"></script>
    <!-- add optional JS plugin files here -->
    <!-- REQUIRED: User Editable JS Files -->
    <script type="text/javascript" src="/Content/assets/js/script.js"></script>
    <!-- add additional User Editable files here -->
    <!-- Demo JS Files -->
    @*Il,Ilce ve Mahalle  de birbirine bağlı gelmeleri için yazdğımız ajax kodları*@
    <script src="~/Content/assets/js/required/jquery-1.11.1.min.js"></script>
    <script>
        function IlceListele() {
            var ilID = $("#IlDropDown").val();

            $.ajax({
                url: '/EmlakOfisi/IlceGetir',
                data: { ilID: ilID },
                type: 'POST',
                dataType: 'json',
                success: function (result) {
                    console.log(result);
                    $("#IlceDropDown").empty();
                    for (var i = 0; i < result.length; i++) {
                        $("#IlceDropDown").append("<option value='" + result[i].Value + "'>" + result[i].Text + "</option>");
                    }
                },
                error: function (exception) {
                    alert(exception);
                },
            });
        }

    </script>
    <script>
        function MahalleListele() {
            var ilceID = $("#IlceDropDown").val();

            $.ajax({
                url: '/EmlakOfisi/MahalleGetir',
                data: { ilceID: ilceID },
                type: 'POST',
                dataType: 'json',
                success: function (result) {
                    console.log(result);
                    $("#MahalleDropDown").empty();
                    for (var i = 0; i < result.length; i++) {
                        $("#MahalleDropDown").append("<option value='" + result[i].Value + "'>" + result[i].Text + "</option>");
                    }
                },
                error: function (exception) {
                    alert(exception);
                },
            });
        }
    </script>

    @*Giris yapılan ikiğ sifrenin aynı olma durumunu kontrol ediyoruz*@
    <script>
        function validate() {
            var x = document.getElementById("inputSifre");
            var y = document.getElementById("inputSifreTekrar");
            var span = document.getElementById("hata");
            var btn = document.getElementById("hesapOlustur");

            if (x.value === y.value) {
                span.innerHTML = "";
                btn.removeAttribute("disabled");
            }
            else {
                span.innerHTML = "Şifreler Uyuşmuyor";
                btn.setAttribute("disabled");
            }
        }

    </script>




</body>
</html>



